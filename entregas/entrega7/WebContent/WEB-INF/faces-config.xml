			<from-outcome>registrado</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inicioMenu</from-outcome>
			<to-view-id>/inicio.xhtml</to-view-id>
		</navigation-case>
